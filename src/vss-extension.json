{
  "manifestVersion": 2,
  "extensionId": "win-rm-file-copy-extended",
  "name": "WinRm File Copy Extended",
  "version": "1.0.2",
  "publisher": "kodaren",
  "description": "Windows Machine File Copy (WinRM) with option to ignore invalid SSL certificates",
  "public": false,
  "galleryFlags": [
    "Public"
  ],
  "icons": {
    "default": "images/logo.png",
    "large": "images/logo-large.png"
  },
  "categories": [
    "Build and release"
  ],
  "tags": [
    "copy",
    "windows",
    "winrm",
    "file"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/kodaren/WinRMFileCopyExtended"
    },
    "support": {
      "uri": "https://github.com/kodaren/WinRMFileCopyExtended"
    },
    "repository": {
        "uri": "https://github.com/kodaren/WinRMFileCopyExtended"
    },
    "issues": {
        "uri": "https://github.com/kodaren/WinRMFileCopyExtended/issues"
    }
  },
  "repository": {
      "type": "git",
      "uri": "https://github.com/kodaren/WinRMFileCopyExtended"
  },
  "branding": {
    "color": "#e9ebed",
    "theme": "light"
  },
  "screenshots": [
    {
        "path": "images/copy-files-winrm.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "WinRMFileCopyExtended"
    }
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "win-rm-file-copy-extended",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "WinRMFileCopyExtended"
      }
    }
  ]
}
